<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="jedd_account_move_form" model="ir.ui.view">
        <field name="name">jedd.account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[1]" position="inside">
                <page name="test_JEDD" string="Test_JEDD" attrs="{'invisible': [('move_type','!=','out_invoice')]}">
                    <group>
                        <field name="use_jedd" widget="boolean_toggle"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>