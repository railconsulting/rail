<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record model="ir.cron" id="send_aged_receivable_report">
    <field name="name">Send Aged Receivable Report to AR Funding</field>
    <field name="model_id" ref="account.model_account_move"/>
    <field name="state">code</field>
    <field name="code">
        model.send_aged_receivable()
    </field>
    <field name='interval_number'>15</field>
    <field name='interval_type'>minutes</field>
    <field name="numbercall">-1</field>
</record>

<record model="ir.cron" id="fetch_invoice_debtor_statuses">
    <field name="name">Fetch Factored Invoice and Debtor Statuses</field>
    <field name="model_id" ref="account.model_account_move"/>
    <field name="state">code</field>
    <field name="code">
        model.fetch_invoice_debtor_statuses()
    </field>
    <field name='interval_number'>1</field>
    <field name='interval_type'>days</field>
    <field name="numbercall">-1</field>
</record>
</odoo>