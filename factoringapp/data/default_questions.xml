<record id="question_3" model="questionnaire.wizard.question">
    <field name="name">Question 3</field>
    <field name="title">What is your company address?</field>
    <field name="question_type">textbox</field>
    <field name="auto_calculate_formula">
        str(record.env.company.street)  + ', ' + str(record.env.company.city) + ' ' +  str(record.env.company.state_id.code) + ' ' +  str(record.env.company.country_id.name)
    </field>
</record>    

<record id="question_4" model="questionnaire.wizard.question">
    <field name="name">Question 4</field>
    <field name="title">Your monthly invoicing based on the past year was automatically pulled from the system.  Please confirm the below amount as correct.</field>
    <field name="question_type">numerical</field>
    <field name="min_validation">25000</field>
    <field name="auto_calculate_formula">
    sum(x.amount_total for x in record.env['account.move'].search([('move_type','=','out_invoice'),('invoice_date','&gt;=',datetime.date(datetime.date.today().year-1,1,1)),('invoice_date','&lt;=',datetime.date(datetime.date.today().year-1,12,31))])) / 12
    </field>
</record> 

